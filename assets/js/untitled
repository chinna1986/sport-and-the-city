var App = App || {};

App.navButton = new function () 
{
	var self = this;
	var global = App;

	self.init = function (){
		$(document.body).on('#nav-btn', 'click', self.go);
	}

	self.go = function (e)
	{
		if (e && e.preventDefault) e.preventDefault();

		return self.inEnd() ? self.prev() : self.next();
	}

	self.inEnd = function ()
	{
		return false;
	}

	self.prev = function ()
	{

	}

	self.next = function ()
	{

	}

}